{
  "name": "jin-frame",
  "version": "3.1.0",
  "description": "Reusable HTTP API request definition library",
  "scripts": {
    "debug": "node --inspect-brk -r ts-node/register ./node_modules/jest/bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "just --config ./.configs/just.config.ts build",
    "rollup:dev": "just --config ./.configs/just.config.ts rollup:dev",
    "rollup:prod": "just --config ./.configs/just.config.ts rollup:prod",
    "pub": "just --config ./.configs/just.config.ts pub",
    "unpub": "just --config ./.configs/just.config.ts unpub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "pub:dev": "just --config ./.configs/just.config.ts pub",
    "clean": "just --config ./.configs/just.config.ts clean",
    "lint": "just --config ./.configs/just.config.ts lint",
    "test:only": "jest --detectOpenHandles --runInBand",
    "test": "cross-env DEBUG=jinframe:* jest --coverage --config jest.config.js",
    "just": "just --config ./.configs/just.config.ts",
    "ctix": "just --config ./.configs/just.config.ts ctix:single",
    "ctix:remove": "just --config ./.configs/just.config.ts ctix:remove",
    "dd": "deps-diff md -d dev -d prod -d peer --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "httpd": "http-server ./docs -p 19983",
    "prepublishOnly": "node prepublish.js"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "typings": "./dist/esm/index.d.mts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "typings": "./dist/esm/index.d.mts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://imjuni.github.io/jin-frame",
  "dependencies": {
    "axios": "1.1.3",
    "dot-prop": "^6.0.1",
    "fast-safe-stringify": "^2.1.1",
    "form-data": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "merge": "^2.1.1",
    "my-easy-fp": "^0.17.0",
    "my-only-either": "^1.3.0",
    "path-to-regexp": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^3.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/clone-deep": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "clone-deep": "^4.0.1",
    "cross-env": "^7.0.3",
    "ctix": "^1.8.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "debug": "^4.3.4",
    "deps-diff": "^1.0.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "gh-pages": "^4.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.3.1",
    "jsonc-parser": "^3.2.0",
    "just-task": "^1.7.0",
    "nock": "^13.2.9",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "read-pkg": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.2",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "directories": {
    "test": "test"
  }
}
