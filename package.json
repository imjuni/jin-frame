{
  "name": "jin-frame",
  "version": "0.13.1",
  "description": "Repeatable HTTP request definition library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "debug": "node --nolazy --require ts-node/register --inspect-brk=9229 ./node_modules/.bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "just build",
    "pub": "just pub",
    "pub:prod": "just pub:prod",
    "pub:dev": "just pub",
    "clean": "just clean",
    "test": "DEBUG=jinframe:* jest",
    "ctix": "ctix create -p ./tsconfig.json"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/imjuni/jin-frame#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "fp-ts": "^2.9.3",
    "http-status-codes": "^1.4.0",
    "my-easy-fp": "^0.5.1",
    "path-to-regexp": "^6.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "ctix": "^0.3.1",
    "debug": "^4.1.1",
    "docsify-cli": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^26.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "jest": "^26.4.2",
    "just-scripts": "^0.44.4",
    "marked": "^1.1.0",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.7",
    "typescript": "^4.1.3",
    "typescript-json-schema": "^0.42.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "directories": {
    "test": "test"
  }
}