{
  "name": "jin-frame",
  "version": "2.1.1",
  "description": "Reusable HTTP API request definition library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "debug": "node --inspect-brk -r ts-node/register ./node_modules/jest/bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "just --config ./.configs/just.config.ts build",
    "webpack:prod": "just --config ./.configs/just.config.ts webpack:prod",
    "dts-bundle": "just --config ./.configs/just.config.ts dts-bundle",
    "pub": "just --config ./.configs/just.config.ts pub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "pub:dev": "just --config ./.configs/just.config.ts pub",
    "clean": "just --config ./.configs/just.config.ts clean",
    "lint": "just --config ./.configs/just.config.ts lint",
    "test": "cross-env DEBUG=jinframe:* jest --config jest.config.js",
    "just": "just --config ./.configs/just.config.ts",
    "ctix": "just --config ./.configs/just.config.ts ctix:single",
    "deploy": "cross-env PUBLIC_URL=\"jin-frame\" gh-pages -d docs",
    "docs": "just --config ./.configs/just.config.ts docs",
    "docs-watch": "just --config ./.configs/just.config.ts  docs-watch",
    "httpd": "http-server ./docs -p 19983"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://imjuni.github.io/jin-frame",
  "dependencies": {
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "dot-prop": "^6.0.1",
    "fast-safe-stringify": "^2.1.1",
    "http-status-codes": "^2.2.0",
    "merge": "^2.1.1",
    "my-easy-fp": "^0.13.0",
    "my-only-either": "^1.2.0",
    "path-to-regexp": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^2.18.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/clone-deep": "^4.0.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/eslint-plugin-tslint": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "clone-deep": "^4.0.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "http-server": "^14.1.1",
    "jest": "^28.1.2",
    "jsonc-parser": "^3.1.0",
    "just-scripts": "^2.0.0",
    "marked": "^4.0.17",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.23.9",
    "typescript": "^4.7.4",
    "typescript-json-schema": "^0.53.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "directories": {
    "test": "test"
  }
}
