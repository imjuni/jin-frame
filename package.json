{
  "name": "jin-frame",
  "version": "3.12.1",
  "description": "Reusable HTTP API request definition library",
  "scripts": {
    "debug": "node --inspect-brk -r ts-node/register ./node_modules/jest/bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "npm run clean && tsc --incremental --project tsconfig.prod.json",
    "dts": "tsc -p tsconfig.dts.json",
    "dts-alias": "tsc-alias -p tsconfig.dts.json",
    "dts-gen": "rollup --config .configs/rollup.config.mjs",
    "dts-pack": "run-s dts dts-alias dts-gen dts-rm",
    "dts-rm": "rimraf dist/types/origin",
    "esbuild-cjs": "FORMAT=cjs node .configs/esbuild.mjs",
    "esbuild-esm": "FORMAT=esm node .configs/esbuild.mjs",
    "bundle-pack": "run-p esbuild-cjs esbuild-esm dts-pack",
    "bundle": "run-s clean ctix bundle-pack ctix-rm",
    "pub": "pnpm run bundle && cross-env RELEASE_MODE=true pnpm publish --registry http://localhost:8901",
    "unpub": "pnpm unpublish $npm_package_name@$npm_package_version --registry http://localhost:8901 --force",
    "pub:prod": "pnpm run bundle && cross-env RELEASE_MODE=true pnpm publish --access=public",
    "clean": "rimraf dist docs",
    "lint": "eslint --cache .",
    "lint-staged": "lint-staged",
    "test:only": "jest --detectOpenHandles --runInBand",
    "test:silent": "LOG_LEVEL=info node --trace-warnings --no-compilation-cache node_modules/.bin/jest --coverage --silent",
    "test": "cross-env DEBUG=jinframe:* node --trace-warnings --no-compilation-cache node_modules/.bin/jest --coverage",
    "docs": "typedoc --options .configs/typedoc.json",
    "ctix": "ctix build --config .configs/.ctirc",
    "ctix-rm": "ctix remove --config .configs/.ctirc --force-yes --remove-backup",
    "dd": "deps-diff md -d dev -d prod -d peer --title-list-type 'm' --deps-list-type 'm' --content-list-type 'm'",
    "httpd": "http-server ./docs -p 19983",
    "prepublishOnly": "node prepublish.cjs",
    "prepare": "husky install"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://imjuni.github.io/jin-frame",
  "dependencies": {
    "axios": "^1.5.1",
    "dot-prop": "^6.0.1",
    "fast-safe-stringify": "^2.1.1",
    "form-data": "^4.0.0",
    "http-status-codes": "^2.2.0",
    "merge": "^2.1.1",
    "my-easy-fp": "^0.21.0",
    "my-only-either": "^1.3.0",
    "path-to-regexp": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^4.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@swc/core": "^1.3.42",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@tsconfig/node16": "^16.1.1",
    "@types/clone-deep": "^4.0.1",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "clone-deep": "^4.0.1",
    "cross-env": "^7.0.3",
    "ctix": "^2.3.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dayjs": "^1.11.9",
    "debug": "^4.3.4",
    "deps-diff": "^1.0.2",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "execa": "^5.1.1",
    "gh-pages": "^6.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest_workaround": "^0.69.0",
    "jsonc-parser": "^3.2.0",
    "lint-staged": "^14.0.1",
    "nock": "^13.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "read-pkg": "^5.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  },
  "directories": {
    "test": "test"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache ."
    ]
  }
}
