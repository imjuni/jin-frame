{
  "name": "jin-frame",
  "version": "0.14.0",
  "description": "Repeatable HTTP request definition library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "debug": "node --nolazy --require ts-node/register --inspect-brk=9229 ./node_modules/.bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "just build",
    "pub": "just pub",
    "pub:prod": "just pub:prod",
    "pub:dev": "just pub",
    "clean": "just clean",
    "test": "DEBUG=jinframe:* jest",
    "ctix": "ctix create -p ./tsconfig.json"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/imjuni/jin-frame#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "fp-ts": "^2.11.0",
    "http-status-codes": "^2.1.4",
    "my-easy-fp": "^0.9.0",
    "path-to-regexp": "^6.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "alias-help": "^0.1.0",
    "ctix": "^0.3.1",
    "debug": "^4.3.2",
    "docsify-cli": "^4.4.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^36.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.0.6",
    "just-scripts": "^1.5.6",
    "marked": "^2.1.3",
    "nock": "^13.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5",
    "typescript-json-schema": "^0.50.1",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "directories": {
    "test": "test"
  }
}