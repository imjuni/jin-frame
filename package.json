{
  "name": "jin-frame",
  "version": "0.17.1",
  "description": "Repeatable HTTP request definition library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "debug": "node --nolazy --require ts-node/register --inspect-brk=9229 ./node_modules/.bin/jest --no-cache --runInBand --detectOpenHandles",
    "build": "just build",
    "pub": "just pub",
    "pub:prod": "just pub:prod",
    "pub:dev": "just pub",
    "clean": "just clean",
    "test": "DEBUG=jinframe:* jest",
    "just": "just",
    "ctix": "ctix create -p ./tsconfig.json"
  },
  "keywords": [
    "axios",
    "decorator",
    "http-client",
    "typescript",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/jin-frame.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/jin-frame/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/imjuni/jin-frame#readme",
  "dependencies": {
    "axios": "^0.26.0",
    "fp-ts": "^2.11.0",
    "http-status-codes": "^2.2.0",
    "luxon": "^2.3.0",
    "my-easy-fp": "^0.10.0",
    "path-to-regexp": "^6.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "alias-help": "^0.1.0",
    "cross-env": "^7.0.3",
    "ctix": "^0.4.1",
    "debug": "^4.3.2",
    "docsify-cli": "^4.4.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^27.5.1",
    "just-scripts": "^1.8.0",
    "marked": "^4.0.12",
    "nock": "^13.2.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5",
    "typescript-json-schema": "^0.50.1",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "directories": {
    "test": "test"
  }
}
